  <div class="d-flex flex-column">
    <%= form_with model: article do |f| %>
    <div class="d-flex flex-column w-25 my-2">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div class="d-flex flex-column w-25 my-2">
      <%= f.label :content %>
      <%= f.text_field :content %>
    </div>
    <div class="mt-2">
      <%= f.submit %>
    </div>
    <% end %>
  </div>
